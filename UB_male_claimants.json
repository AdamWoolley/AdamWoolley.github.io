{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "https://api.allorigins.win/raw?url=https://api.ons.gov.uk/timeseries/DPAE/dataset/LMS/data",
    "format": {"type": "json", "property": "years"}
  },
  "title": {
    "text": "UK Male Unemployment Benefit",
    "subtitle": "United Kingdom (thousands, seasonally adjusted) claimants",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
  },
  "width": 200,
  "height": 200,
  "mark": {"type": "line", "color": "darkred"},
  "encoding": {
    "x": {"field": "year", "type": "temporal", "title": "Date"},
    "y": {"field": "value", "type": "quantitative", "title": "Claimants"}
  },
  "config": {}
}